{
  "type": "db_query",
  "params":
  [
    { "name": "measurement", "type": "measurement", "options": "", "from_context": false, "mandatory": true },
    { "name": "year1", "type": "year", "options": "begin", "from_context": false, "mandatory": false },
    { "name": "year2", "type": "year", "options": "end", "from_context": false, "mandatory": false },
    { "name": "space_agency", "type": "space_agency", "options": "", "from_context": false, "mandatory": false }
  ],
  "query":
  {
    "always": "session.query(models.Instrument.name).join(models.Mission, models.Instrument.missions).group_by(models.Instrument.name).filter(models.Instrument.measurements.any(models.Measurement.name.ilike('%${measurement}%')))",
    "opt":
    [
      { "cond": "year1", "query_part": ".having(func.max(models.Mission.eol_date) > data['year1'])" },
      { "cond": "year2", "query_part": ".having(func.min(models.Mission.launch_date) < data['year2'])" },
      { "cond": "space_agency", "query_part": ".filter(models.Mission.agencies.any(models.Agency.name.ilike('%${space_agency}%')))" }
    ],
    "end": ".order_by(func.min(models.Mission.launch_date))",
    "results": [
      {
        "result_type": "list",
        "result_fields": {
          "name": "row.name"
        }
      }
    ]
  },
  "voice_response": [
    {
      "type": "list",
      "begin": "The instruments that measure ${measurement} are ",
      "repeat": "${name}",
      "end": "."
    }
  ],
  "visual_response": [
    {
      "type": "text",
      "from": "list",
      "begin": "The instruments that measure ${measurement} are ",
      "repeat": "${name}",
      "end": "."
    }
  ]
}