{
  "type": "run_function",
  "params": [],
  "objective": "Determine the driving features in a set of designs",
  "function":
  {
    "run_template": "dialogue_functions.analyst.data_mining_run(context, user_information, session)",
    "results": [
      {
        "result_type": "list",
        "result_fields": {
          "adv_type": "item[\"type\"]",
          "advice": "item[\"advice\"]"
        }
      }
    ]
  },
  "voice_response": [
    {
      "type": "list",
      "begin": "The driving features of the target designs are: ",
      "repeat": "${advice}",
      "end": "."
    }
  ],
  "visual_response": [
    {
      "type": "list",
      "begin": "The driving features of the target designs are: ",
      "item_template": "<b>${advice}"
    }
  ]
}