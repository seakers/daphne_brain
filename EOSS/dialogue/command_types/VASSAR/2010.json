{
  "type": "run_function",
  "params":
  [
    { "name": "instrument_parameter", "type": "instrument_parameter", "options": "", "from_context": false, "mandatory": true },
    { "name": "vassar_measurement", "type": "vassar_measurement", "options": "", "from_context": false, "mandatory": true }
  ],
  "function":
  {
    "run_template": "dialogue_functions.engineer.get_measurement_requirement('${vassar_measurement}', '${instrument_parameter}', context)",
    "results": [
      {
        "result_type": "single",
        "result_fields": {
          "text_response": "command_result"
        }
      }
    ]
  },
  "voice_response": [
    {
      "type": "single",
      "template": "${text_response}"
    }
  ],
  "visual_response": [
    {
      "type": "text",
      "from": "single",
      "template": "${text_response}"
    }
  ]
