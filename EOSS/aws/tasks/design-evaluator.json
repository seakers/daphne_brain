{
    "task_definition": {
        "family":  "user-n-design-evaluator-task",
        "executionRoleArn":  "arn:aws:iam::923405430231:role/ecsTaskExecutionRole",
        "taskRoleArn":  "arn:aws:iam::923405430231:role/DaphneEvaluatorTaskRole",
        "cpu":  "1024",
        "memory":  "4096",
        "networkMode":  "awsvpc",
        "containerDefinitions":  [
            {
                "name": "user-n-design-evaluator-task-definition",
                "image": "923405430231.dkr.ecr.us-east-2.amazonaws.com/design-evaluator:latest",
                "cpu": 1024,
                "memory": 4096,
                "memoryReservation": 2048,
                "essential": true,
                "command": [
                    "/app/evaluator/bin/evaluator"
                ],
                "environment": [
                    {"name": "DEPLOYMENT_TYPE", "value": "AWS"},
                    {"name": "JAVA_OPTS", "value": "-Xmx3840m"},
                    {"name": "APOLLO_URL", "value": "http://graphql.daphne.dev:8080/v1/graphql"},
                    {"name": "APOLLO_URL_WS", "value": "ws://graphql.daphne.dev:8080/v1/graphql"}
                ],
                "logConfiguration": {
                    "logDriver": "awslogs",
                    "options": {
                      "awslogs-group": "daphne-evaluator",
                      "awslogs-region": "us-east-2",
                      "awslogs-create-group": "true",
                      "awslogs-stream-prefix": "daphne"
                    }
                }
            }
        ],
        "requiresCompatibilities":  [
            "FARGATE"
        ]
    },
    "task_instance": {
        "cluster": "daphne-dev-cluster",
        "count": 1,
        "launchType": "FARGATE",
        "networkConfiguration": {
            "awsvpcConfiguration": {
                "subnets": [
                    "subnet-05cc334fd084cb66c",
                    "subnet-0131f6edbbd6f6bbd"
                ],
                "securityGroups": [
                    "sg-03871503ca9368508"
                ],
                "assignPublicIp": "DISABLED"
            }
        },
        "overrides": {
            "containerOverrides": [
                {
                    "name": "user-n-design-evaluator-task",
                    "environment": [
                        {"name": "PING_REQUEST_URL", "value": "OVERRIDE"},
                        {"name": "PING_RESPONSE_URL", "value": "OVERRIDE"},
                        {"name": "PRIVATE_REQUEST_URL", "value": "OVERRIDE"},
                        {"name": "PRIVATE_RESPONSE_URL", "value": "OVERRIDE"},
                        {"name": "EVAL_REQUEST_URL", "value": "OVERRIDE"},
                        {"name": "EVAL_RESPONSE_URL", "value": "OVERRIDE"}
                    ]
                }
            ]
        },
        "propagateTags": "TASK_DEFINITION",
        "tags" :  [
            {"key": "PING_REQUEST_URL", "value": "OVERRIDE"},
            {"key": "PING_RESPONSE_URL", "value": "OVERRIDE"},
            {"key": "PRIVATE_REQUEST_URL", "value": "OVERRIDE"},
            {"key": "PRIVATE_RESPONSE_URL", "value": "OVERRIDE"}
        ],
        "taskDefinition": "user-1-design-evaluator-task"
    }
}