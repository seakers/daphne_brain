{
  "type": "run_function",
  "params":
  [
    { "name": "current_scorecard_path", "type": "edl_mat_file", "options": "", "mandatory": true, "from_context": true, "context": "edlcontext", "subcontext": "" },
    { "name": "current_mat_file", "type": "edl_mat_file", "options": "", "mandatory": true, "from_context": true, "context": "edlcontext", "subcontext": ""}
  ],
  "function":
  {
    "run_template": "run_func.edl.get_flag_summary('None', '${current_mat_file}', context, 'outofspec_results')",
    "results": [
      {
        "result_type": "list",
        "result_fields": {
        "load_result": "item['command_result']"
        }
      }
    ]
  },
  "voice_response": [
    {
     "type": "list",
      "begin": "The out of spec metrics in this scorecard are",
      "repeat": "${load_result}",
      "end":"."
    }
  ],
  "visual_response": [
     {
    "type": "list",
    "begin": "The out of spec metrics in this scorecard are",
    "item_template": "${load_result}"
  }
  ]
}