{
  "type": "run_function",
  "params":
  [
    { "name": "anomaly", "type": "anomaly", "options": "", "from_context": false, "mandatory": true }
  ],
  "objective": "Provide the risks of a certain anomaly",
  "function":
  {
    "run_template": "dialogue_functions.get_anomaly_risks(data['anomaly'])",
    "results": [
      {
        "result_type": "list",
        "result_fields": {
          "risk": "item"
        }
      }
    ]
  },
  "voice_response": [
    {
      "type": "list",
      "begin": "The risks for the ${anomaly} anomaly are:",
      "repeat": "${risk}",
      "end": "."
    }
  ],
  "visual_response": [
    {
      "type": "list",
      "begin": "The risks for the ${anomaly} anomaly are: ",
      "item_template": "${risk}"
    }
  ]
}