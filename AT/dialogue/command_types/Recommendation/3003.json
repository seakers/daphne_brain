{
  "type": "run_function",
  "params":
  [
    { "name": "anomaly", "type": "anomaly", "options": "", "from_context": false, "mandatory": true }
  ],
  "objective": "Provide the estimated time of resolution for a certain anomaly.",
  "function":
  {
    "run_template": "dialogue_functions.get_anomaly_etr(data['anomaly'])",
    "results": [
      {
        "result_type": "list",
        "result_fields": {
          "procedure_name": "item['procedure_name']",
          "procedure_time": "item['procedure_time']"
        }
      }
    ]
  },
  "voice_response": [
    {
      "type": "list",
      "begin": "The ${anomaly} anomaly estimated resolution time break down is:",
      "repeat": "${procedure_name}: ${procedure_time} [min]",
      "end": "."
    }
  ],
  "visual_response": [
    {
      "type": "list",
      "begin": "The ${anomaly} anomaly estimated resolution time break down is:",
      "item_template": "${procedure_name}: ${procedure_time} [min]"
    }
  ]
}