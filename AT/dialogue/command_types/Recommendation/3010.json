{
  "type": "run_function",
  "params":
  [
    { "name": "procedure_number", "type": "procedure_number", "options": "", "from_context": false, "mandatory": false }
  ],
  "objective": "Provide a procedure using the procedure number.",
  "function":
  {
    "run_template": "dialogue_functions.get_procedure_from_number(data['procedure_number'])",
    "results": [
      {
        "result_type": "single",
        "result_fields": {
          "pdf_link": "command_results[0]['pdf_link']",
          "full_procedure": "command_results[0]['procedure_name']"
        }
      }
    ]
  },
  "voice_response": [
    {
      "type": "single",
      "template": "Here's a link to the procedure."
    }
  ],
  "visual_response": [
    {
      "type": "text",
      "from": "single",
      "template": "Here's a link to the procedure: <a href=\"api/at/recommendation/procedure?${pdf_link}\" target=\"_blank\">${full_procedure}</a>."
    }
  ]
}