{
  "type": "run_function",
  "params":
  [
    { "name": "procedureNumber", "type": "procedureNumber", "options": "", "from_context": false, "mandatory": true },
    { "name": "step_number", "type": "step_number", "options": "", "from_context": false, "mandatory": true }
  ],
  "objective": "Provide a step using procedure name.",
  "function":
  {
    "run_template": "dialogue_functions.get_step_from_procedure_name(data['step_number'], data['procedureNumber'])",
    "results": [
      {
        "result_type": "single",
        "result_fields": {
          "step": "command_results[0]['step']",
          "action": "command_results[0]['action']"
        }
      }
    ]
  },
  "voice_response": [
    {
      "type": "single",
      "template": "${step} of the procedure is, ${action}."
    }
  ],
  "visual_response": [
    {
      "type": "text",
      "from": "single",
      "template": "${step} of the procedure is \n${action}"
    }
  ]
}